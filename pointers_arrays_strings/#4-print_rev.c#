#include "main.h"
/**
 * rev_string - print a reverse string
 * @s: int
 *
 * Return: void
 *
 */
void rev_string(char *s)
{
	char m;
	char *j;
	char *i;

for (i = s; *i != '\0'; ++i)
;
if (i > s)
--i;

	for (j = s; j < i; ++j, --i)
	{
	m = *j;
	*j = *i;
	*i = m;
	}
}#include "main.h"
#include <string.h>
/**
 *print_rev - prints a reverse string
 *@s: string
 *
 *Return: void
 */
void print_rev(char *s)
{
	int c = 0;
	int size = strlen(s);

	for (c = 0; c < size / 2; c++)
	{
		swap(s[c], s[size - c -1 ]);
	}
}
